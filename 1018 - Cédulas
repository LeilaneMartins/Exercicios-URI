#include <stdio.h>
 
int main() {
    int x, resto, cem, cinquenta, vinte, dez, cinco, dois, um;

    scanf("%i",&x);

    resto = x;

    cem = x/100;
    resto = resto - (cem * 100);

    cinquenta = resto/50;
    resto = resto - (cinquenta * 50);

    vinte = resto/20;
    resto = resto - (vinte * 20);

    dez = resto/10;
    resto = resto - (dez * 20);

    cinco = resto/5;
    resto = resto - (cinco * 5);

    dois = resto/2;
    resto = resto - (dois * 2);

    printf("%i\n",x);
    printf("%i nota(s) de R$ 100,00\n",cem);
    printf("%i nota(s) de R$ 50,00\n",cinquenta);
    printf("%i nota(s) de R$ 20,00\n",vinte);
    printf("%i nota(s) de R$ 10,00\n",dez);
    printf("%i nota(s) de R$ 5,00\n",cinco);
    printf("%i nota(s) de R$ 2,00\n",dois);
    printf("%i nota(s) de R$ 1,00\n",resto);
 
    return 0;
}
